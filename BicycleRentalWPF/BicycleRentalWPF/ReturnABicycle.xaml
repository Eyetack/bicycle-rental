<Window x:Class="BicycleRentalWPF.ReturnABicycle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ReturnABicycle" Height="300" Width="458">
    <Grid>
        <Label Content="Return Bicycle" HorizontalAlignment="Left" Margin="184,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.342,-0.231" Width="89"/>
        <Label Content="Choose Bicycle To Return: " HorizontalAlignment="Left" Margin="57,90,0,0" VerticalAlignment="Top" Width="148" RenderTransformOrigin="0.649,0.154"/>
        <ComboBox x:Name="BicycleReturnBox" HorizontalAlignment="Left" Margin="231,94,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="BicycleReturnBox_SelectionChanged"/>
        <Button x:Name="SubmitButton" Content="SUBMIT" HorizontalAlignment="Left" Margin="119,170,0,0" VerticalAlignment="Top" Width="75" Click="SubmitButton_Click"/>
        <Button x:Name="BackButton" Content="BACK" HorizontalAlignment="Left" Margin="276,170,0,0" VerticalAlignment="Top" Width="75" Height="20" Click="BackButton_Click"/>

    </Grid>
</Window>
